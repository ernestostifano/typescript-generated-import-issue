{
    "name": "@scope/package-b-import",
    "version": "1.0.0-beta.1",
    "types": "./types/index.d.ts",
    "module": "./build/index.js",
    "main": "./build/index.js",
    "exports": {
        ".": {
            "types": "./types/index.d.ts",
            "import": "./build/index.js",
            "require": "./build/index.js",
            "default": "./build/index.js"
        }
    },
    "files": [
        "build",
        "types"
    ],
    "scripts": {
        "build": "yarn run build:code && yarn run build:types",
        "build:code": "babel --root-mode upward ./src --out-dir ./build --copy-files --source-maps --extensions \".ts,.js\"",
        "build:types": "tsc --project ./tsconfig.json"
    },
    "dependencies": {
        "@scope/package-a-import": "workspace:*",
        "@scope/package-b-export": "workspace:*"
    },
    "devDependencies": {
        "@babel/cli": "^7.21.0",
        "@babel/core": "^7.21.0",
        "typescript": "^4.9.5"
    }
}
